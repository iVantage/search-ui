{
  "name": "@ivantagehealth/search-ui-app-search-connector",
  "version": "1.0.1",
  "description": "A Search UI connector for Elastic's App Search Service",
  "license": "Apache-2.0",
  "main": "lib",
  "module": "es",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivantage/search-ui.git",
    "directory": "packages/search-ui-app-search-connector"
  },
  "scripts": {
    "test-ci": "jest --runInBand",
    "test": "jest",
    "clean": "rimraf es lib",
    "watch-js": "babel src --out-dir lib --ignore '**/*.test.js' --watch & babel src --out-dir es --ignore '**/*.test.js' --watch &",
    "build-js": "babel src --out-dir lib --ignore '**/*.test.js' && babel src --out-dir es --ignore '**/*.test.js'",
    "build": "run-s clean build-js",
    "watch": "run-p watch-js",
    "prepare": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/ivantage/search-ui/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "@elastic/app-search-javascript": "^7.3.0"
  }
}
